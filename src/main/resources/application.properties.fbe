spring.jpa.generate-ddl=true
spring.jpa.hibernate.ddl-auto=none


management.endpoints.web.exposure.include=metrics

spring.datasource.url: ${vcap.services.postgresql.credentials.jdbcUrl:jdbc:postgresql:mem:postgres}
spring.datasource.username: ${vcap.services.postgresql.credentials.username:postgres}
spring.datasource.password: ${vcap.services.postgresql.credentials.password:}
spring.datasource.testOnBorrow: true
